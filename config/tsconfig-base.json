{
  "compilerOptions": {
    "lib": [ "ES2020" ],
    "module": "ES2020",
    "target": "ES2020",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowUnreachableCode": false,
    "skipLibCheck": true,
    "sourceMap": true,
    "alwaysStrict": true,
    "strict": true,
    "incremental": true,
    "composite": true
  },
  "exclude": [ "node_modules" ]
}
